<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<Appenders>
<Console name="ConsoleAppender" target="SYSTEM_OUT">
    <PatternLayout pattern="[%style{%date{DEFAULT}}{yellow}] [%logger{36}] [%highlight{%level}{FATAL=bg_red, ERROR=red, WARN=yellow, INFO=green}] - %message%n"/>
</Console>
</Appenders>
<Loggers>
<Root level="DEBUG">
  <AppenderRef ref="ConsoleAppender"/>
</Root>
</Loggers>
</Configuration>